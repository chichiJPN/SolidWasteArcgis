@{
    ViewBag.Title = "Map";
}

@section CssHeader {
    <link rel="stylesheet" href="https://js.arcgis.com/3.18/esri/css/esri.css">
}
<div id="dataDiv" class="display-none" data-district-url="@Url.Action("getDistricts", "ajax")"></div>
<div id="viewDiv"></div>


@section ScriptsFooter {
    <script src="https://js.arcgis.com/3.18/" type="text/javascript"></script>
    <script> window.baseUrl = "@string.Format("{0}://{1}{2}", Request.Url.Scheme, Request.Url.Authority, Url.Content("~"))"; </script>
    <script src="@Url.Content("~/Scripts/arcgis-map.js")" type="text/javascript"></script>


}
